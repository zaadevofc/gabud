---
import "../styles/global.css";
import { AlignLeft } from "lucide-astro";
---

<!doctype html>
<html lang="en" data-theme="cupcake">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Gabud | Open Source Projects</title>
    <meta name="description" content="-" />

    <meta property="og:url" content="https://zaadevofc.tech" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="Gabud | Open Source Projects" />
    <meta property="og:description" content="-" />
    <meta
      property="og:image"
      content="https://zaadevofc.tech/assets/zaadevofc-icon-black-white.png"
    />
    <meta property="og:image:width" content="470" />
    <meta property="og:image:height" content="470" />

    <meta name="twitter:card" content="summary_large_image" />
    <meta property="twitter:domain" content="zaadevofc.tech" />
    <meta property="twitter:url" content="https://zaadevofc.tech" />
    <meta name="twitter:title" content="Gabud | Open Source Projects" />
    <meta name="twitter:description" content="-" />
    <meta
      name="twitter:image"
      content="https://zaadevofc.tech/assets/zaadevofc-icon-black-white.png"
    />

    <link rel="apple-touch-icon" sizes="180x180" href="/gabud/apple-touch-icon.png" />
    <link rel="icon" type="image/png" sizes="32x32" href="/gabud/favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="/gabud/favicon-16x16.png" />
    <link rel="manifest" href="/gabud/site.webmanifest" />
    <link rel="mask-icon" href="/gabud/safari-pinned-tab.svg" color="#5bbad5" />
    <meta name="msapplication-TileColor" content="#2b5797" />
    <meta name="theme-color" content="#ffffff" />

    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
      rel="stylesheet"
    />
  </head>
  <body>
    <main class="flex flex-col mx-auto w-full max-w-screen-2xl">
      <div class="navbar bg-base-100">
        <div class="navbar-start">
          <div class="dropdown">
            <div tabindex="0" role="button" class="btn btn-ghost btn-circle">
              <AlignLeft />
            </div>
            <ul
              tabindex="0"
              class="menu menu-sm dropdown-content mt-3 z-[1] p-2 shadow bg-base-100 rounded-box w-52"
            >
              <li><a>Homepage</a></li>
              <li><a>Portfolio</a></li>
              <li><a>About</a></li>
            </ul>
          </div>
        </div>
        <div class="navbar-center">
          <a class="btn btn-ghost text-xl font-bold">Gabud.</a>
        </div>
        <div class="navbar-end">
          <button class="btn btn-ghost btn-circle">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              class="h-5 w-5"
              fill="none"
              viewBox="0 0 24 24"
              stroke="currentColor"
              ><path
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="2"
                d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"></path></svg
            >
          </button>
          <button class="btn btn-ghost btn-circle">
            <div class="indicator">
              <svg
                xmlns="http://www.w3.org/2000/svg"
                class="h-5 w-5"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
                ><path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M15 17h5l-1.405-1.405A2.032 2.032 0 0118 14.158V11a6.002 6.002 0 00-4-5.659V5a2 2 0 10-4 0v.341C7.67 6.165 6 8.388 6 11v3.159c0 .538-.214 1.055-.595 1.436L4 17h5m6 0v1a3 3 0 11-6 0v-1m6 0H9"
                ></path></svg
              >
              <span class="badge badge-xs badge-primary indicator-item"></span>
            </div>
          </button>
        </div>
      </div>
    </main>
  </body>
</html>
